{
  "editor.wordWrap": "on",
  "chat.editor.wordWrap": "on",
  "notebook.output.wordWrap": true,
  "editor.tabSize": 2,
  "editor.tabCompletion": "on",
  "files.autoSave": "afterDelay",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.iconTheme": "material-icon-theme",
  "git.autofetch": true,
  "terminal.integrated.env.windows": {},
  "console-ninja.featureSet": "Community",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "git.confirmSync": false,
  "vercelVSCode.accessToken": "",
  "vercelVSCode.projectId": "",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.enableExperiments": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always",
    "source.fixAll": "always"
  },
  "workbench.colorTheme": "Shades of Purple (Super Dark)",
  "files.dialog.defaultPath": "F:\\Coding\\VSCode",
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "console-ninja.fontSize": 14,
  "files.associations": {
    "*.tailwind.css": "tailwindcss",
    "*.tailwind.scss": "tailwindcss"
  },
  "workbench.editor.wrapTabs": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "oracledevtools.connectionConfiguration.configFilesFolder": "",
  "[oraclesql]": {
    "editor.suggest.showSnippets": true,
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.defaultFormatter": "Oracle.oracledevtools"
  },
  "oracledevtools.connectionConfiguration.walletFileFolder": "",
  "oracledevtools.bookmarkFileFolder": "",
  "oracledevtools.download.otherFolder": ""
}
